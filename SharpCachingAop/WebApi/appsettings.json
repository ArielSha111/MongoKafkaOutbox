{
  "AppSettings": {
    "DatabaseConnectionString": "Data Source=(localdb)\\TestDB;Initial Catalog=master;Integrated Security=True;MultipleActiveResultSets=True; MultiSubnetFailover=True;App=TestApp"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "CachingConfigurationBlock": {
    "DeadLockTimeOut": "00:10:00",
    "ItemsConfiguration": {
      "GetData": {
        "Duration": "00:10:00",
        "ReturnDeepCopy": true
      },
      "GetStuffFromDB": {
        "Duration": "00:05:00",
        "ReturnDeepCopy": true
      }
    }
  }
}
